{
	"configurations": [
		{
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}"
			],
			"name": "Launch Client",
			"outFiles": [
				"${workspaceRoot}/client/out/**/*.js"
			],
			"preLaunchTask": {
				"script": "watch",
				"type": "npm"
			},
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"type": "extensionHost"
		}
	],
	"version": "0.2.0"
}